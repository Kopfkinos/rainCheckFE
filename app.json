{
  "expo": {
    "name": "rainCheckFE",
    "slug": "rainCheckFE",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/app-icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/images/rainCheck-logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.kopfkinos.rainCheckFE.androidBuild"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/images/rainCheck-logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/images/rainCheck-logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-secure-store",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "127f0b19-e698-4d7f-9ac4-9d6c6ffe4826"
      }
    }
  }
}
